@startuml
title Uttak, konsesjoner og bergrettigheter - Objekttyper

skinparam backgroundColor #F7F8FA
skinparam shadowing false
skinparam RoundCorner 8
skinparam ArrowColor #94A3B8
skinparam class {
  BackgroundColor #FFFFFF
  BorderColor #CBD5E1
  FontColor #0F172A
  HeaderBackgroundColor #EEF2F7
  HeaderFontColor #0F172A
  AttributeIconSize 0
}
skinparam stereotypeCBackgroundColor #E2E8F0
skinparam stereotypeCBorderColor #CBD5E1
skinparam stereotypeCFontColor #0F172A

package "Objekttyper" {

  class Undersoekelsesrett <<featureType>> {
    + geometry [1] : GM_MultiSurface
    + Rettighetsnummer [0..1] : CharacterString
    + Rettighetsnavn [0..1] : CharacterString
    + Rettighetstype [0..1] : CharacterString
    + Rettighetshaver [0..1] : CharacterString
    + Organisasjonsnummer [0..1] : CharacterString
    + Status [0..1] : CharacterString
    + Mottatt [0..1] : Date
    + Godkjent [0..1] : Date
    + Mineral [0..1] : CharacterString
    + Kommunenummer [0..1] : CharacterString
    + Kommunenavn [0..1] : CharacterString
    + Fylkesnummer [0..1] : CharacterString
    + Fylkesnavn [0..1] : CharacterString
    + Areal_m2 [0..1] : CharacterString
    + Årsavgift_nok [0..1] : int
    + Årsavgift_arealgrunnlag [0..1] : int
    + Årsavgift_kalenderår [0..1] : int
    + Årsavgift_sats [0..1] : int
  }

  class Utvinningsrett <<featureType>> {
    + geometry [1] : GM_MultiSurface
    + Rettighetsnummer [0..1] : CharacterString
    + Rettighetsnavn [0..1] : CharacterString
    + Rettighetstype [0..1] : CharacterString
    + Rettighetshaver [0..1] : CharacterString
    + Organisasjonsnummer [0..1] : CharacterString
    + Status [0..1] : CharacterString
    + Mottatt [0..1] : Date
    + Godkjent [0..1] : Date
    + Mineral [0..1] : CharacterString
    + Kommunenummer [0..1] : CharacterString
    + Kommunenavn [0..1] : CharacterString
    + Fylkesnummer [0..1] : CharacterString
    + Fylkesnavn [0..1] : CharacterString
    + Areal_m2 [0..1] : CharacterString
    + Årsavgift_nok [0..1] : int
    + Årsavgift_arealgrunnlag [0..1] : int
    + Årsavgift_kalenderår [0..1] : int
    + Årsavgift_sats [0..1] : int
  }

  class Undersoekelsesrapport <<featureType>> {
    + geometry [1] : GM_MultiPoint
    + Rapportnummer [0..1] : CharacterString
    + Rapporttittel [0..1] : Boolean
    + Kommunenummer [0..1] : CharacterString
    + Kommunenavn [0..1] : CharacterString
    + Fylkesnummer [0..1] : CharacterString
    + Fylkesnavn [0..1] : CharacterString
  }

  class Konsesjon <<featureType>> {
    + geometry [1] : GM_MultiSurface
    + Konsesjonsnummer [0..1] : CharacterString
    + Uttaksnummer [0..1] : int
    + Uttaksnavn [0..1] : CharacterString
    + Konsesjonær [0..1] : CharacterString
    + Organisasjonsnummer [0..1] : CharacterString
    + Foretaksnavn [0..1] : CharacterString
    + Kommunenummer [0..1] : CharacterString
    + Kommunenavn [0..1] : CharacterString
    + Fylkesnummer [0..1] : CharacterString
    + Fylkesnavn [0..1] : CharacterString
  }

  class Aktsomhetssone <<featureType>> {
    + geometry [1] : GM_Surface
    + Sikringstype [0..1] : CharacterString
    + Sikringsnummer [0..1] : int
    + Sikringsnavn [0..1] : CharacterString
    + Kommunenummer [0..1] : int
    + Kommunenavn [0..1] : CharacterString
    + Fylkesnummer [0..1] : int
    + Fylkesnavn [0..1] : CharacterString
  }

  class Uttak <<featureType>> {
    + geometry [1] : GM_Point
    + Uttaksnummer [0..1] : int
    + Uttaksnavn [0..1] : CharacterString
    + Driftsstatus [0..1] : CharacterString
    + Produktgruppe [0..1] : CharacterString
    + Produkt [0..1] : CharacterString
    + Tiltakshaver [0..1] : CharacterString
    + Organisasjonsnummer [0..1] : int
    + Foretaksnavn [0..1] : CharacterString
    + ØSS [0..1] : Boolean
    + Driftsrapport [0..1] : Boolean
    + Kommunenummer [0..1] : int
    + Kommunenavn [0..1] : CharacterString
    + Fylkesnummer [0..1] : int
    + Fylkesnavn [0..1] : CharacterString
  }
}

@enduml
